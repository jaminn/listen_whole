<!DOCTYPE html>
<html lang="en">

<head>
    <title>List_EN</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="/javascripts/socket.io-1.4.5.js"></script>
    <script src="/javascripts/jquery-1.11.3.min.js"></script>
    <link rel="stylesheet" href="/stylesheets/bootstrap.min.css">
    <script src="/javascripts/bootstrap.min.js"></script>
    <script src="/javascripts/srt__.js"></script>
    <script>
        $(document).ready(function() {
            $("#starter").hide();
        });
        setInterval(function() {
            if ($("#away").html().includes("id")) {
                $("#starter").show();
            }
        }, 1000);
        var socket = io.connect();
        var room = "<%=room%>"
        var name = "<%=name%>"
        var vid = "<%=vid%>"
        var level = "<%=level%>"

        socket.emit('connectRoom', room);
        socket.emit('sendAlert', name, "/multi_wait_away/" + vid + "/" + room);
        socket.emit('get');
        socket.emit('info', "home", name, level);

        socket.on('home', function(homeUser, level) {
            $('#home').html("<h1>id:" + homeUser + "</h1>" + "<hr/>" + "<h2>level:" + level + "</h2>");
        });

        socket.on('away', function(awayUser, level) {
            $('#away').html("<h1>id:" + awayUser + "</h1>" + "<hr/>" + "<h2>level:" + level + "</h2>");
        });

        socket.on('start', function(name, vid, room){
          location.replace("/multi_home/"+name+"/"+room+"/vid?vid="+vid);
        })

        function go() {
          socket.emit('go', name, vid, room);
        }
    </script>

    <style>
        body {
            background-color: #f0f0f0;
            margin-top: 50px;
        }
        /* Remove the navbar's default margin-bottom and rounded borders */

        .navbar {
            margin-bottom: 0;
            border-radius: 0;
            background-color: #1976D2;
            color: #000;
            border: none;
        }

        #login {
            color: #FFF;
        }

        #login:hover {
            color: #000;
        }

        #ov {
            color: #FFF;
        }

        #ov:hover {
            color: #9d9d9d;
        }

        #exam {
            margin-top: 10px;
            width: 1150px;
            height: 700px;
            background-color: #FFF;
            border-radius: 20px;
            box-shadow: 0 0 5px 0 #BDBDBD;
        }

        @media (max-width: 767px) {
            .gl {
                font-size: 17px;
                font-weight: bold;
                text-decoration: none;
                text-align: center;
                border-bottom: 3px solid #AAA;
                padding-bottom: 6px;
            }
        }
    </style>
</head>

<body>


    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand3" href="/" class="pull-left"><img src="/images/Blogo.png" height="30px" /></a>
                <a class="navbar-brand2" href="#" id="login"><span class="glyphicon glyphicon-log-in"></span> <%=name%>&nbsp;&nbsp;&nbsp;&nbsp;</a>
            </div>
            <div class="collapse navbar-collapse " id="myNavbar">

                <ul class="nav navbar-nav">
                    <li><a href="/media" id="ov"><i class="glyphicon glyphicon-home"></i> &nbsp;홈</a></li>
                    <li><a href="#" id="ov" onclick="alert('서비스 준비 중입니다')"><i class="glyphicon glyphicon-time"></i> &nbsp;복습하기</a></li>
                </ul>
            </div>
        </div>
    </nav>

    <article>
        <section>
            <div class="text-center">
                <div class="row">
                    <div class="col-sm-4">.
                        <div class="thumbnail">
                            <img src="/uploaded/default.jpg" height="80px" style="border-radius:300px;">
                            <div class="caption">
                                <h1>home</h1>
                                <hr/>
                                <div id="home">

                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="thumbnail">
                            <div id=Awayprofile>
                                <img data-src="holder.js/300x200" alt="300x200" src="data:image/png;base64,"
                                    style="width: 300px; height: 200px;">
                            </div>
                            <div class="caption">
                                <h1>away</h1>
                                <hr/>
                                <div id="away">

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="well well-lg">
                    <span class="gl">
      			<h4 id="wait">사람을기달리는중입니다</h4>
			</span></div>
                <button type="button" id="starter" class="btn btn-warning" style="font-weight:bold;font-size:20px;margin: 30px auto; padding:20px 50px;border-radius:10px;" onclick="go()">시작!</button>
            </div>
        </section>
    </article>

</body>

</html>
